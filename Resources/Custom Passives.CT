        <CheatEntry>
          <ID>106</ID>
          <Description>"Player Summon"</Description>
          <Options moHideChildren="1"/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>/*
How to use this script :
https://www.youtube.com/watch?v=BeWccO06N80&amp;list=PL03h91jY8sapT1dknpUtyBPvi0hum_vZt
*/

[ENABLE]
//active when open Abilearn Board
//close-open Abilearn Board for refresh Passive
//use for Player summon from spirit
aobscanmodule(INJECTpassivetype,nie.exe,42 8B 9C AD * * 00 00)
alloc(newmem,$1000,INJECTpassivetype)
label(code passiveTypeAdr mempassivetypeBck)
label(return)
newmem:
  db 4A 8D 9C
  readMem(INJECTpassivetype+3,5)
  push rax
  push r8
  mov r8,passiveTypeAdr
  lea rax,[r8+r13*8]
  mov [rax],rbx
  pop r8
  pop rax
code:
  readMem(INJECTpassivetype,8)
  jmp return
mempassivetypeBck:
  readMem(INJECTpassivetype,8)
passiveTypeAdr:
  dq 0
INJECTpassivetype:
  jmp newmem
  nop 3
return:
registersymbol(INJECTpassivetype)
registersymbol(mempassivetypeBck passiveTypeAdr)
[DISABLE]
INJECTpassivetype:
  readMem(mempassivetypeBck,8)
unregistersymbol(INJECTpassivetype)
unregistersymbol(mempassivetypeBck passiveTypeAdr)
dealloc(newmem)

