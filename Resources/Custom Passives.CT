        <CheatEntry>
          <ID>118</ID>
          <Description>"Player Hero, Fabled"</Description>
          <Options moHideChildren="1"/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>/*
How to use this script :
https://www.youtube.com/watch?v=BeWccO06N80&amp;list=PL03h91jY8sapT1dknpUtyBPvi0hum_vZt
*/

[ENABLE]
//active when open Abilearn Board
aobscanmodule(INJECTpassivetypehero,nie.exe,45 85 FF 75 03 4? 8B 3?)
alloc(newmem,$1000,INJECTpassivetypehero)
label(code codeF mempassivetypeheroBck passiveTypeHeroAdr)
label(cfPassivetypeheroFabled)
label(return)
newmem:
  jne return
  mov r15,rsi
  cmp edi,1
  jne @f
  mov [passiveTypeHeroAdr],r15
@@:
  cmp edi,3
  jne @f
  mov [passiveTypeHeroAdr+8],r15
@@:
  cmp [cfPassivetypeheroFabled],1
  je codeF
  cmp edi,7
  jne @f
  mov [passiveTypeHeroAdr+10],r15
@@:
  cmp edi,B
  jne @f
  mov [passiveTypeHeroAdr+18],r15
@@:
  cmp edi,E
  jne code
  mov [passiveTypeHeroAdr+20],r15
  jmp code
codeF:
//Fabled
  cmp edi,8
  jne @f
  mov [passiveTypeHeroAdr+10],r15
@@:
  cmp edi,C
  jne @f
  mov [passiveTypeHeroAdr+18],r15
@@:
  cmp edi,F
  jne code
  mov [passiveTypeHeroAdr+20],r15
code:
  readMem(INJECTpassivetypehero+5,3)
  jmp return
mempassivetypeheroBck:
  readMem(INJECTpassivetypehero+3,5)
cfPassivetypeheroFabled:
  dd 0 //Hero-Type (0 = Hero, 1 = Fabled)
passiveTypeHeroAdr:
  dq 0
INJECTpassivetypehero+3:
  jmp newmem
return:
registersymbol(INJECTpassivetypehero)
registersymbol(mempassivetypeheroBck passiveTypeHeroAdr cfPassivetypeheroFabled)
[DISABLE]
INJECTpassivetypehero+3:
  readMem(mempassivetypeheroBck,5)
unregistersymbol(INJECTpassivetypehero)
unregistersymbol(mempassivetypeheroBck passiveTypeHeroAdr cfPassivetypeheroFabled)
dealloc(newmem)

